    "@types/loopback": "^3.2.6",
    "@types/loopback-boot": "^2.23.4",
    "@types/node": "^10.12.1",
    "@types/pino": "^5.20.0",
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "@types/webpack": "^4.4.17",
    "@types/webpack-merge": "^4.1.3",
    "autoprefixer": "^9.3.1",
    "awesome-typescript-loader": "^5.2.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
