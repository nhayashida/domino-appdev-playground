    "@slack/interactive-messages": "^1.0.2",
    "ajv": "^6.9.2",
    "carbon-components": "^9.81.1",
    "carbon-components-react": "^6.98.2",
    "carbon-icons": "^7.0.7",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^6.2.0",
    "helmet": "^3.15.1",
    "lodash": "^4.17.11",
    "loopback": "^3.25.0",
    "loopback-boot": "^2.27.1",
    "moment": "^2.24.0",
    "pino": "^5.11.1",
    "pino-pretty": "^2.5.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "strong-error-handler": "^3.2.0",
    "tsscmp": "^1.0.6",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@fimbul/ymir": "^0.18.0",
