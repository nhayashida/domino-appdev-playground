    "ajv": "^6.5.4",
    "carbon-components": "^9.42.3",
    "carbon-components-react": "^6.45.2",
    "carbon-icons": "^7.0.7",
    "classnames": "^2.2.6",
    "compression": "^1.7.3",
    "cors": "^2.8.4",
    "helmet": "^3.14.0",
    "lodash": "^4.17.11",
    "loopback": "^3.23.2",
    "loopback-boot": "^2.27.1",
    "pino": "^5.8.0",
    "pino-pretty": "^2.2.3",
    "querystring": "^0.2.0",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "strong-error-handler": "^3.2.0"
