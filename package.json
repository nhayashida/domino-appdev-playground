{
  "name": "proton-dql-playground",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm run lint && rimraf ./dist && tsc && npm run copy && webpack --config webpack.prod.ts",
    "build:dev": "rimraf ./dist && tsc && npm run copy && webpack --config webpack.dev.ts",
    "start": "cross-env NODE_ENV=production node dist/server/server.js",
    "start:dev": "cross-env NODE_ENV=development nodemon --exec node --inspect -r ts-node/register src/server/server.ts",
    "copy": "cpx \"./src/server/*.json\" ./dist/server/"
  },
  "dependencies": {
    "@domino/domino-db": "file:domino-domino-db-1.0.0.tgz",
