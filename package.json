    "@types/webpack-merge": "^4.1.3",
    "cache-loader": "^2.0.1",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
