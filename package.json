{
  "name": "domino-appdev-playground",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhayashida/domino-appdev-playground.git"
  },
  "scripts": {
    "env": "cross-env CARBON_USE_EXPERIMENTAL_FEATURES=true CARBON_USE_BREAKING_CHANGES=true",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "build": "npm run env && npm run lint && rimraf ./dist && tsc && npm run copy && webpack --config webpack.prod.ts",
    "build:dev": "npm run env && rimraf ./dist && tsc && npm run copy && webpack --config webpack.dev.ts",
    "start": "cross-env NODE_ENV=production node -r dotenv/config dist/server/server.js",
    "copy": "cpx \"./src/common/models/*.json\" ./dist/common/models && cpx \"./src/server/*.json\" ./dist/server/"
  },
  "dependencies": {
    "@carbon/icons-react": "0.0.1-alpha.12",
    "@domino/domino-db": "file:domino-domino-db-1.1.0.tgz",
